<?xml version="1.0" encoding="UTF-8"?>
<xd:project xmlns:xd="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
	<xd:modelVersion>4.0.0</xd:modelVersion>
	<xd:parent>
		<xd:groupId>org.springframework.boot</xd:groupId>
		<xd:artifactId>spring-boot-starter-parent</xd:artifactId>
		<xd:version>3.1.4</xd:version>
		<xd:relativePath/>
	</xd:parent>
	<xd:groupId>com.TOTeams</xd:groupId>
	<xd:artifactId>TeacherHub</xd:artifactId>
	<xd:version>0.0.1-SNAPSHOT</xd:version>
	<xd:name>TeacherHub</xd:name>
	<xd:description>Api rest for TeacherHub app</xd:description>
	<xd:properties>
		<xd:java.version>21</xd:java.version>
	</xd:properties>
	<xd:repositories>
		<xd:repository>
			<xd:id>postgresql</xd:id>
			<xd:url>https://repo.maven.apache.org/maven2/org/postgresql/</xd:url>
		</xd:repository>
	</xd:repositories>
	<xd:dependencies>
		<xd:dependency>
			<xd:groupId>org.postgresql</xd:groupId>
			<xd:artifactId>postgresql</xd:artifactId>
			<xd:scope>runtime</xd:scope>
		</xd:dependency>

		<xd:dependency>
			<xd:groupId>org.springframework.boot</xd:groupId>
		</xd:dependency>
		<xd:dependency>
			<xd:groupId>org.springframework.boot</xd:groupId>
			<xd:artifactId>spring-boot-starter-data-jpa</xd:artifactId>
		</xd:dependency>
		<xd:dependency>
			<xd:groupId>org.springframework.boot</xd:groupId>
			<xd:artifactId>spring-boot-starter-security</xd:artifactId>
		</xd:dependency>
		<xd:dependency>
			<xd:groupId>org.springframework.boot</xd:groupId>
			<xd:artifactId>spring-boot-starter-web</xd:artifactId>
		</xd:dependency>
		<xd:dependency>
			<xd:groupId>org.springframework.boot</xd:groupId>
			<xd:artifactId>spring-boot-devtools</xd:artifactId>
			<xd:scope>runtime</xd:scope>
			<xd:optional>true</xd:optional>
		</xd:dependency>
		<xd:dependency>
			<xd:groupId>com.mysql</xd:groupId>
			<xd:artifactId>mysql-connector-j</xd:artifactId>
			<xd:scope>runtime</xd:scope>
		</xd:dependency>
		<xd:dependency>
      		<xd:groupId>org.postgresql</xd:groupId>
      		<xd:artifactId>postgresql</xd:artifactId>
      		<xd:scope>runtime</xd:scope>
   		</xd:dependency>
		<xd:dependency>
			<xd:groupId>org.projectlombok</xd:groupId>
			<xd:artifactId>lombok</xd:artifactId>
			<xd:optional>true</xd:optional>
		</xd:dependency>
		<xd:dependency>
			<xd:groupId>org.springframework.boot</xd:groupId>
			<xd:artifactId>spring-boot-starter-test</xd:artifactId>
			<xd:scope>test</xd:scope>
		</xd:dependency>
		<xd:dependency>
			<xd:groupId>org.springframework.security</xd:groupId>
			<xd:artifactId>spring-security-test</xd:artifactId>
			<xd:scope>test</xd:scope>
		</xd:dependency>
			<xd:dependency>
			<xd:groupId>io.jsonwebtoken</xd:groupId>
			<xd:artifactId>jjwt-api</xd:artifactId>
			<xd:version>0.11.5</xd:version>
		</xd:dependency>
		<xd:dependency>
			<xd:groupId>io.jsonwebtoken</xd:groupId>
			<xd:artifactId>jjwt-impl</xd:artifactId>
			<xd:version>0.11.5</xd:version>
			<xd:scope>runtime</xd:scope>
		</xd:dependency>
		<xd:dependency>
			<xd:groupId>io.jsonwebtoken</xd:groupId>
			<xd:artifactId>jjwt-jackson</xd:artifactId>
			<xd:version>0.11.5</xd:version>
			<xd:scope>runtime</xd:scope>
		</xd:dependency>

	</xd:dependencies>

	<xd:build>
		<xd:plugins>
			<xd:plugin>
				<xd:groupId>org.springframework.boot</xd:groupId>
				<xd:artifactId>spring-boot-maven-plugin</xd:artifactId>
				<xd:configuration>
					<xd:excludes>
						<xd:exclude>
							<xd:groupId>org.project-lombok</xd:groupId>
							<xd:artifactId>lombok</xd:artifactId>
						</xd:exclude>
					</xd:excludes>
				</xd:configuration>
			</xd:plugin>
		</xd:plugins>
	</xd:build>

</xd:project>
